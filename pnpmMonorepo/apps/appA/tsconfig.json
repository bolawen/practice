{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "baseUrl": "./src",
      "paths": {
        "*": ["*", "src/*"],
        // "utils": ["../../libs/utils/src"],
        // "components": ["../../libs/components/src"]
      },
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true
    },
    "include": ["src/**/*.ts", "src/**/*.tsx"],
    "exclude": ["node_modules", "dist"]
  }
  